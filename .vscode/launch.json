    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "InCusterConfig": "false",
                "ActiveDeadlineSecond": "4200",
                "GetPodLogs": "true",
                "LogTail": "100",
                "CleanFailedJob": "true",
                "DeleteJob": "true",
                "DeletePod": "true",
                "ExcludedNamespaces": "databases"
            },
            "args": []
        }
    ]
}